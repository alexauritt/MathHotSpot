- content_for :head do
  %title Grouped Math Problems
  = stylesheet_link_tag "admin/math_problems"

%h2 Grouped Math Problems
- @problem_groups.each do |group|
  .index_group_container_odd
    .problem_instruction 
      = group.first.instruction.description
    - group.each do |problem|
      .problem
        = render(:partial => "problem_display", :locals => { :math_problem => problem })