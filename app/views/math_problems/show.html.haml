- content_for :head do
  %title Math Problem
  = stylesheet_link_tag "admin/math_problems"

%p
  %b Question:
  =display_mathml_question(@math_problem)
  %b Answer:
  =display_mathml_answer(@math_problem)


%p
  %b Question:
  =@math_problem.question_markup
  
%p
  %b Answer:
  =@math_problem.answer_markup

=link_to 'Edit', edit_math_problem_path(@math_problem)
| 
=link_to 'Back to Level', problem_type_problem_level_url(@math_problem.problem_type.permalink, @math_problem.problem_level.level_number)
