- content_for :head do
  %title Rougue Math Problems
  = stylesheet_link_tag "admin/math_problems"

%h2 Rougue Math Problems
- @rougue_problems.each do |problem|
  .problem_instruction
    - if problem.instruction
      = group.first.instruction.description
  .problem
    = render(:partial => "problem_display", :locals => { :math_problem => problem })