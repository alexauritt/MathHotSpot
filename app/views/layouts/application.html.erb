<!DOCTYPE html>
<html>
<head>
  <title>Mathhotspot</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "/javascripts/mathjax/MathJax.js?config=MML_HTMLorMML-full" %>
  <%= javascript_include_tag("jquery.throbber.min.js") %>
  <%= csrf_meta_tag %>
  <%= yield :head %>
</head>
<body>
  <% if flash[:notice] %>
    <p class="notice"><%= flash[:notice] %></p>
  <% end %>
  <%= yield %>
</body>
</html>
